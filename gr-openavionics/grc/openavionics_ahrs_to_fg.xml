<?xml version="1.0"?>
<block>
  <name>ahrs_to_fg</name>
  <key>openavionics_ahrs_to_fg</key>
  <category>openavionics</category>
  <import>import openavionics</import>
  <make>openavionics.ahrs_to_fg($ip_addr,$port)</make>
  <param>
    <name>IP Address</name>
    <key>ip_addr</key>
    <type>string</type>
  </param>
  
<param>
    <name>Port</name>
    <key>port</key>
    <type>int</type>
  </param>

   <sink>
    <name>in</name>
    <type>message</type>
    <optional>1</optional>
  </sink>

</block>
